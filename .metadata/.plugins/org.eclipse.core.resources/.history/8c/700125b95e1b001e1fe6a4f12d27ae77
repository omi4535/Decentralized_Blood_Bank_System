package com.blood_bank_system.tables;

import java.sql.Date;

public class inventory {
    private int inventoryId;
    private int bloodBankId;
    private int groupId;
    private String bloodGroup;
    private int availableUnits;
    private Date lastUpdatedDate;
    private String storageLocation;

    public inventory(int inventoryId, int bloodBankId, int groupId, String bloodGroup, int availableUnits, Date lastUpdatedDate, String storageLocation) {
        this.inventoryId = inventoryId;
        this.bloodBankId = bloodBankId;
        this.groupId = groupId;
        this.bloodGroup = bloodGroup;
        this.availableUnits = availableUnits;
        this.lastUpdatedDate = lastUpdatedDate;
        this.storageLocation = storageLocation;
    }
    
    public inventory( int bloodBankId, int groupId, String bloodGroup, int availableUnits, Date lastUpdatedDate, String storageLocation) {
//        this.inventoryId = inventoryId;
        this.bloodBankId = bloodBankId;
        this.groupId = groupId;
        this.bloodGroup = bloodGroup;
        this.availableUnits = availableUnits;
        this.lastUpdatedDate = lastUpdatedDate;
        this.storageLocation = storageLocation;
    }

    public int getInventoryId() {
        return inventoryId;
    }

    public void setInventoryId(int inventoryId) {
        this.inventoryId = inventoryId;
    }

    public int getBloodBankId() {
        return bloodBankId;
    }

    public void setBloodBankId(int bloodBankId) {
        this.bloodBankId = bloodBankId;
    }

    public int getGroupId() {
        return groupId;
    }

    public void setGroupId(int groupId) {
        this.groupId = groupId;
    }

    public String getBloodGroup() {
        return bloodGroup;
    }

    public void setBloodGroup(String bloodGroup) {
        this.bloodGroup = bloodGroup;
    }

    public int getAvailableUnits() {
        return availableUnits;
    }

    public void setAvailableUnits(int availableUnits) {
        this.availableUnits = availableUnits;
    }

    public Date getLastUpdatedDate() {
        return lastUpdatedDate;
    }

    public void setLastUpdatedDate(Date lastUpdatedDate) {
        this.lastUpdatedDate = lastUpdatedDate;
    }

    public String getStorageLocation() {
        return storageLocation;
    }

    public void setStorageLocation(String storageLocation) {
        this.storageLocation = storageLocation;
    }
}


